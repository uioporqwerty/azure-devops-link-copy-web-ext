{
	"manifest_version": 2,
	"version": "0.0.1",
	"name": "Azure DevOps Link Copy",
	"short_name": "ADO Link Copy",
	"description": "Quickly copy the link to any Azure DevOps work item",
	"developer": {
		"name": "Nitish Sachar",
		"url": "https://github.com/uioporqwerty/azure-devops-link-copy-web-ext"
	},
	"icons": {
		"64": "icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://dev.azure.com/*"
			],
			"js": [
				"content_script.js"
			]
		}
	]
}